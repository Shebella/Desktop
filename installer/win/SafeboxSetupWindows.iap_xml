strQuery = "SELECT * FROM Win32_NetworkAdapterConfiguration WHERE MACAddress > ''"


Set objWMIService = GetObject( "winmgmts://./root/CIMV2" )
Set colItems      = objWMIService.ExecQuery( strQuery, "WQL", 48 )

For Each objItem In colItems
If IsArray( objItem.IPAddress ) Then
        If UBound( objItem.IPAddress ) = 0  Then
            ipAddress =   objItem.IPAddress(0) 
	    macAddress =  "???"

        Else
            ipAddress =   objItem.IPAddress(0) 
	    macAddress =  objItem.MACAddress
        End If
	exit for
    End If

Next


ver="Safebox"
if WScript.Arguments.count >=3  then ver=WScript.Arguments.item(1)

opt="unknown"
if WScript.Arguments.count >=3  then opt=WScript.Arguments.item(2)



rslt="perpared"


os=wshShell.ExpandEnvironmentStrings( "%OS%" )


cmpnm= wshShell.ExpandEnvironmentStrings( "%COMPUTERNAME%" ) 
cmpnm= cmpnm & "." & Info.DomainDNSName
osAct = wshShell.ExpandEnvironmentStrings( "%USERNAME%" )

n_now=now()
 set oShell = CreateObject("WScript.Shell") 
atb = "HKEY_LOCAL_MACHINE\System\CurrentControlSet\" &_ 
        "Control\TimeZoneInformation\ActiveTimeBias" 
offsetMin = oShell.RegRead(atb) 
n = dateadd("n", offsetMin, n_now) 



logtime = year(n) & "-" & right( "0" & month(n),2) & "-" & right("0" & day(n),2)  & " " &  right("0" &hour(n),2) & ":" & right ("0" &minute(n),2)  & ":" & right("0" & second(n),2) 

srvIp="192.168.214.12"
if WScript.Arguments.count >=3  then srvIp=WScript.Arguments.item(0)




Set http = CreateObject("Microsoft.xmlhttp")
http.Open "GET", "http://" & srvIp & ":8088/sbx_svr/test.jsp",False

http.setRequestHeader "Content-Type", "text/html"
http.setRequestHeader "Log-type", "installation" 
http.send logtime & vbTab & cmpnm & vbTab & macAddress & vbTab & os & vbTab & osAct & vbTab & ipAddress & vbTab & srvIp & vbTab & ver & vbTab & opt & vbTab & rslt
